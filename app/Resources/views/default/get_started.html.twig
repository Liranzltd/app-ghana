{% extends 'base.html.twig' %}
{% block stylesheets %}
{{ parent() }}
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css" integrity="sha384-HSMxcRTRxnN+Bdg0JdbxYKrThecOKuH5zCYotlSAcp1+c8xmyTe9GYg1l9a69psu" crossorigin="anonymous">
<link rel="stylesheet" href="/bundles/app/css/style.css?ver=4">
<style>
.platinum {
  background-image: url('data:image/svg+xml;base64,') !important
}
.bronze {
  background-image: url('data:image/svg+xml;base64,')
  }

  .silver {
    background-image: url('data:image/svg+xml;base64,')
  }

  .gold {
    background-image: url('data:image/svg+xml;base64,')
    }
.pricing-tables {
  margin: 50px auto;
  width: 100%;
  font-family: "Open Sans", sans-serif;
  font-size: 14px;
}
.pricing-table {
  float: left;
  width: 25%;
  margin: 0 auto;
  padding: 0;
  background-color: white;
  position: relative;
  border-radius: 10px;
  box-shadow: 0px 0px 20px silver;
  z-index: 0;
  box-sizing: border-box;
  min-height: 850px;
  background-position: 1% 20%;
  background-size: contain;
  background-repeat: no-repeat;
  border: none !important;
}
.pop-out {
  transform: scale(1.05);
  transition: 0.3s;
  z-index: 2;
  box-shadow: 0px 0px 20px silver;
}
.pricing-table li {
  list-style: none;
  line-height: 2em;
  text-align: left;
  padding: 7px;
}
.pricing-table .title {
  font-weight: bold;
  background-color: #FFF;
  border-radius: 8px 8px 0 0;
  font-size: 1.1em;
  border: none;
  text-align: center;
  text-transform: capitalize;
}
.pricing-table .title span
{
  display: block;
    font-weight: normal;
}
.pricing-table .price {
  font-size: 1.6em;
  border: none;
  text-align: center;
}
.pricing-table .description {
  font-size: 0.9em;
  color: #666;
  border: none;
  font-style: italic;
  text-align: center;
}
.pricing-table .deliverables {
  font-weight: bold;
  font-size: 0.9em;
}
.pricing-table .cta-button {
  border: none;
  border-radius: 0 0 9px 9px;
  background-color: #008fef;
  padding: 0;
  position: absolute;
/* border: 0px; */
  width: 100%;
  bottom: 0px;
}
.started {
  display: block;
  width: 100%;
  line-height: 4;
  color: #FFF;
  font-weight: bold;
  text-align: center;
}
.cta-button:hover {
  background-color: #CCC;
  transition-duration: 0.3s;
}
#international .pricing-table
{
  width: 45%;
}
</style>
{% endblock %}
  {% block body %}
  <div class="container">
    <section class="profile-company-content main-user pt60">
      <ul class="nav nav-pills">
        <li class="nav-item">
            <a href="#local" class="nav-link active" data-toggle="tab">Join as Local Suppliers</a>
        </li>
        <li class="nav-item">
            <a href="#international" class="nav-link" data-toggle="tab">Join as an International Suppliers</a>
        </li>
    </ul>
    <div class="tab-content">
        <div class="tab-pane fade in active" id="local">
          <div class="row">
            <div class="pricing-tables">
            {% for tier in tiers %}
            {% if tier.membershipCategory == "Local Suppliers" %}
              <ul class="pricing-table {{ tier.name|lower }}">
                <li class="title">{{ tier.name|lower }} <span>{{ tier.membershipCategory   }}</a></li>
                <li class="price">GHS {{ tier.subscriptionFee|number_format(0, '.', ',') }} / year</li>
                <li class="description">{{ tier.description }} </li>
                {% for benefit in tier.benefits %}
                  <li>{{ benefit.title|raw }}</li>
                {% endfor %}
                <li class="cta-button"><a href="{{ path('user-register',{'t':tier.id}) }}" class="started">Get Started</a></li>
              </ul>
              {% endif %}
            {% endfor %}
            </div>
          </div>
        </div>
        <div class="tab-pane fade" id="international">
          <div class="row">
            <div class="pricing-tables">
          {% for tier in tiers|sort|reverse %}
          {% if tier.membershipCategory == "International Suppliers" %}
            <ul class="pricing-table {{ tier.name|lower }}">
              <li class="title">{{ tier.name|lower }} <span>{{ tier.membershipCategory   }}</a></li>
              <li class="price">GHS {{ tier.subscriptionFee|number_format(0, '.', ',') }} / year</li>
              <li class="description">{{ tier.description }} </li>
              {% for benefit in tier.benefits %}
                <li>{{ benefit.title|raw }}</li>
              {% endfor %}
              <li class="cta-button"><a href="{{ path('user-register',{'t':tier.id}) }}" class="started">Get Started</a></li>
            </ul>
            {% endif %}
          {% endfor %}
          </div>
          </div>
        </div>
    </div>
    </section>
  </div>
{% endblock %}
