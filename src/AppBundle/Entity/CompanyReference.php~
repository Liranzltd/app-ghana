<?php

namespace AppBundle\Entity;

/**
 * CompanyReference
 */
class CompanyReference
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $client;

    /**
     * @var string
     */
    private $contactPerson;

    /**
     * @var string
     */
    private $email;

    /**
     * @var string
     */
    private $telephone;

    /**
     * @var \AppBundle\Entity\Company
     */
    private $company;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set client
     *
     * @param string $client
     *
     * @return CompanyReference
     */
    public function setClient($client)
    {
        $this->client = $client;

        return $this;
    }

    /**
     * Get client
     *
     * @return string
     */
    public function getClient()
    {
        return $this->client;
    }

    /**
     * Set contactPerson
     *
     * @param string $contactPerson
     *
     * @return CompanyReference
     */
    public function setContactPerson($contactPerson)
    {
        $this->contactPerson = $contactPerson;

        return $this;
    }

    /**
     * Get contactPerson
     *
     * @return string
     */
    public function getContactPerson()
    {
        return $this->contactPerson;
    }

    /**
     * Set email
     *
     * @param string $email
     *
     * @return CompanyReference
     */
    public function setEmail($email)
    {
        $this->email = $email;

        return $this;
    }

    /**
     * Get email
     *
     * @return string
     */
    public function getEmail()
    {
        return $this->email;
    }

    /**
     * Set telephone
     *
     * @param string $telephone
     *
     * @return CompanyReference
     */
    public function setTelephone($telephone)
    {
        $this->telephone = $telephone;

        return $this;
    }

    /**
     * Get telephone
     *
     * @return string
     */
    public function getTelephone()
    {
        return $this->telephone;
    }

    /**
     * Set company
     *
     * @param \AppBundle\Entity\Company $company
     *
     * @return CompanyReference
     */
    public function setCompany(\AppBundle\Entity\Company $company = null)
    {
        $this->company = $company;

        return $this;
    }

    /**
     * Get company
     *
     * @return \AppBundle\Entity\Company
     */
    public function getCompany()
    {
        return $this->company;
    }
    /**
     * @var string
     */
    private $description;

    /**
     * @var string
     */
    private $valueOfContract;

    /**
     * @var string
     */
    private $currency;

    /**
     * @var string
     */
    private $file;

    /**
     * @var string
     */
    private $designation;


    /**
     * Set description
     *
     * @param string $description
     *
     * @return CompanyReference
     */
    public function setDescription($description)
    {
        $this->description = $description;

        return $this;
    }

    /**
     * Get description
     *
     * @return string
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * Set valueOfContract
     *
     * @param string $valueOfContract
     *
     * @return CompanyReference
     */
    public function setValueOfContract($valueOfContract)
    {
        $this->valueOfContract = $valueOfContract;

        return $this;
    }

    /**
     * Get valueOfContract
     *
     * @return string
     */
    public function getValueOfContract()
    {
        return $this->valueOfContract;
    }

    /**
     * Set currency
     *
     * @param string $currency
     *
     * @return CompanyReference
     */
    public function setCurrency($currency)
    {
        $this->currency = $currency;

        return $this;
    }

    /**
     * Get currency
     *
     * @return string
     */
    public function getCurrency()
    {
        return $this->currency;
    }

    /**
     * Set file
     *
     * @param string $file
     *
     * @return CompanyReference
     */
    public function setFile($file)
    {
        $this->file = $file;

        return $this;
    }

    /**
     * Get file
     *
     * @return string
     */
    public function getFile()
    {
        return $this->file;
    }

    /**
     * Set designation
     *
     * @param string $designation
     *
     * @return CompanyReference
     */
    public function setDesignation($designation)
    {
        $this->designation = $designation;

        return $this;
    }

    /**
     * Get designation
     *
     * @return string
     */
    public function getDesignation()
    {
        return $this->designation;
    }
    /**
     * @var \AppBundle\Entity\CompanyDocumentation
     */
    private $document;


    /**
     * Set document
     *
     * @param \AppBundle\Entity\CompanyDocumentation $document
     *
     * @return CompanyReference
     */
    public function setDocument(\AppBundle\Entity\CompanyDocumentation $document = null)
    {
        $this->document = $document;

        return $this;
    }

    /**
     * Get document
     *
     * @return \AppBundle\Entity\CompanyDocumentation
     */
    public function getDocument()
    {
        return $this->document;
    }
}
